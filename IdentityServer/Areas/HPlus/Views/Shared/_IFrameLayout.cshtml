<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - H+</title>

    <partial name="_BootstrapStylesPartial" />
    <partial name="_AnimateCssStylesPartial" />
    <partial name="_LayuiStylesPartial" />

    <link href="~/library/h-plus/css/font-awesome.css" rel="stylesheet" />
    <link href="~/library/h-plus/css/style.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>
    @RenderBody()
    <partial name="_JQueryScriptsPartial" />
    <partial name="_BootstrapScriptsPartial" />
    <partial name="_LayuiScriptsPartial" />

    @*<script src="~/library/h-plus/js/plugins/metisMenu/jquery.metisMenu.js"></script>*@
    @*<script src="~/library/h-plus/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>*@

    @*<script src="~/library/h-plus/js/hplus.js"></script>
    <script src="~/library/h-plus/js/contabs.js"></script>*@

    @*<script src="~/library/h-plus/js/plugins/pace/pace.min.js"></script>*@

    @RenderSection("Scripts", required: false)
</body>
</html>
